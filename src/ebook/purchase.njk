<script>
  document.addEventListener('alpine:init', () => {
    Alpine.data('ebookPurchase', () => ({
      email: '',
      firstName: '',
      lastName: '',
      loading: false,
      isLoggedIn: false,

      init() {
        // Check if user is logged in and prefill email
        if (Alpine.store('auth')?.isAuthenticated) {
          this.isLoggedIn = true;
          this.email = Alpine.store('auth').userEmail || '';
          this.firstName = Alpine.store('auth').user?.user_metadata?.first_name || '';
          this.lastName = Alpine.store('auth').user?.user_metadata?.last_name || '';
        }
      },

      async processPurchase() {
        if (!this.email) {
          alert('Please enter your email address');
          return;
        }

        this.loading = true;

        try {
          const headers = {
            'Content-Type': 'application/json'
          };

          // Include auth token if user is logged in
          if (this.isLoggedIn && Alpine.store('auth')?.session?.access_token) {
            headers['Authorization'] = `Bearer ${Alpine.store('auth').session.access_token}`;
          }

          const response = await fetch('/api/stripe-create-session', {
            method: 'POST',
            headers,
            body: JSON.stringify({
              email: this.email,
              firstName: this.firstName,
              lastName: this.lastName
            })
          });

          const data = await response.json();

          if (response.ok && data.checkout_url) {
            // Redirect to Stripe Checkout
            window.location.href = data.checkout_url;
          } else {
            console.error('Purchase error:', data);
            alert('Failed to create checkout session. Please try again.');
          }
        } catch (error) {
          console.error('Purchase error:', error);
          alert('An error occurred. Please try again.');
        } finally {
          this.loading = false;
        }
      }
    }));
  });
</script> 